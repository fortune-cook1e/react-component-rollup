  "scripts": {
    "dev:page": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "build:page": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "dev:component": "cross-env NODE_ENV=production rollup -c -w",
    "build:component": "cross-env rm -rf lib && NODE_ENV=production rollup -c",
    "dev:story": "start-storybook -p 6006",
    "build:story": "build-storybook",
    "lint": "cross-env eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "stylelint": "stylelint src *.less,*.css --fix",
    "pre-publish": "yarn run build:component"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "yarn lint",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
